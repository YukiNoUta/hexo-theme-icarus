#cover-backstretch
    position: fixed
    left: 0
    top: 0
    width: 100%
    height: 100%
    background-position: center
    background-size: cover
    &.lazyload
      &:not(.loaded)
        opacity: 0
      &.loaded
        animation-delay: 0s
        animation-duration: .5s
        animation-fill-mode: forwards
        animation-timing-function: ease-out
        animation-name: fadeIn
        @keyframes fadeIn
          0%
            opacity: 0
            filter blur(12px)
            transform: scale(1.02)
          100%
            opacity: 1